{
  "name": "relient-admin-template",
  "version": "0.0.1",
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@babel/polyfill": "^7.10.4",
    "antd": "^4.5.2",
    "bignumber.js": "^9.0.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.5",
    "date-fns": "^2.15.0",
    "echarts": "^4.8.0",
    "express": "^4.16.4",
    "final-form": "^4.20.1",
    "history": "^5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-style-loader": "^5.1.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.5.1",
    "react-images": "^1.1.7",
    "react-photo-gallery": "^8.0.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "relient": "4.4.1",
    "relient-admin": "2.1.2",
    "universal-router": "^9.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-lodash": "^3.3.2",
    "enzyme": "^3.3.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "react-deep-force-update": "^2.1.1",
    "react-test-renderer": "^16.8.6",
    "relient-cli": "2.7.8",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*.{css,less,styl,scss,sass,sss}": "stylelint --fix"
  },
  "scripts": {
    "lint-js": "eslint --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-js && yarn run fix-css",
    "check": "flow check",
    "test": "yarn run lint",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "clean": "relient clean",
    "copy": "relient copy",
    "bundle": "relient bundle",
    "build": "relient build",
    "build-stats": "relient build --analyse",
    "serve": "relient serve",
    "start": "relient start",
    "mocker": "relient mocker"
  }
}
